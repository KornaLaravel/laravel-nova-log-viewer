{
  "name": "laravel/nova-log-viewer",
  "description": "A Laravel Nova tool for viewing logs.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Laravel\\Nova\\LogViewer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Laravel\\Nova\\LogViewer\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
